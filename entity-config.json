{
  "entities": [
    {
      "name": "userApp",
      "description": "User entity representing system users",
      "fields": [
        {
          "name": "username",
          "type": "String",
          "description": "Unique username",
          "required": true
        },
        {
          "name": "email",
          "type": "String",
          "description": "User email address",
          "required": true
        },
        {
          "name": "password",
          "type": "String",
          "description": "Encrypted password",
          "required": true
        },
        {
          "name": "firstName",
          "type": "String",
          "description": "User first name",
          "required": true
        },
        {
          "name": "lastName",
          "type": "String",
          "description": "User last name",
          "required": true
        },
        {
          "name": "birthDate",
          "type": "LocalDate",
          "description": "User birth date",
          "required": false
        },
        {
          "name": "active",
          "type": "Boolean",
          "description": "Is user active",
          "required": false
        }
      ]
    },
    {
      "name": "product",
      "description": "Product entity for e-commerce",
      "fields": [
        {
          "name": "name",
          "type": "String",
          "description": "Product name",
          "required": true
        },
        {
          "name": "description",
          "type": "String",
          "description": "Product description",
          "required": false
        },
        {
          "name": "price",
          "type": "Double",
          "description": "Product price",
          "required": true
        },
        {
          "name": "quantity",
          "type": "Integer",
          "description": "Available quantity",
          "required": true
        },
        {
          "name": "category",
          "type": "String",
          "description": "Product category",
          "required": true
        },
        {
          "name": "imageUrl",
          "type": "String",
          "description": "Product image URL",
          "required": false
        }
      ]
    },
    {
      "name": "orderApp",
      "description": "Order entity for purchases",
      "fields": [
        {
          "name": "orderDate",
          "type": "LocalDateTime",
          "description": "Order creation date",
          "required": true
        },
        {
          "name": "totalAmount",
          "type": "Double",
          "description": "Total order amount",
          "required": true
        },
        {
          "name": "status",
          "type": "String",
          "description": "Order status",
          "required": true
        },
        {
          "name": "customer",
          "type": "UserApp",
          "description": "Order customer",
          "required": true,
          "relation": {
            "type": "MANY_TO_ONE",
            "targetEntity": "userApp",
            "fetchType": "LAZY",
            "cascadeType": "PERSIST"
          }
        },
        {
          "name": "orderItems",
          "type": "List<OrderItem>",
          "description": "Order items",
          "required": false,
          "relation": {
            "type": "ONE_TO_MANY",
            "targetEntity": "orderItem",
            "fetchType": "LAZY",
            "cascadeType": "ALL"
          }
        }
      ]
    },
    {
      "name": "orderItem",
      "description": "Order item entity",
      "fields": [
        {
          "name": "quantity",
          "type": "Integer",
          "description": "Item quantity",
          "required": true
        },
        {
          "name": "unitPrice",
          "type": "Double",
          "description": "Unit price at time of order",
          "required": true
        },
        {
          "name": "product",
          "type": "Product",
          "description": "Ordered product",
          "required": true,
          "relation": {
            "type": "MANY_TO_ONE",
            "targetEntity": "product",
            "fetchType": "EAGER",
            "cascadeType": "PERSIST"
          }
        },
        {
          "name": "order",
          "type": "OrderApp",
          "description": "Parent order",
          "required": true,
          "relation": {
            "type": "MANY_TO_ONE",
            "targetEntity": "orderApp",
            "fetchType": "LAZY",
            "cascadeType": "PERSIST"
          }
        }
      ]
    },
    {
      "name": "category",
      "description": "Product category",
      "fields": [
        {
          "name": "name",
          "type": "String",
          "description": "Category name",
          "required": true
        },
        {
          "name": "description",
          "type": "String",
          "description": "Category description",
          "required": false
        },
        {
          "name": "parentCategory",
          "type": "Category",
          "description": "Parent category",
          "required": false,
          "relation": {
            "type": "MANY_TO_ONE",
            "targetEntity": "category",
            "fetchType": "LAZY",
            "cascadeType": "PERSIST"
          }
        },
        {
          "name": "products",
          "type": "List<Product>",
          "description": "Category products",
          "required": false,
          "relation": {
            "type": "ONE_TO_MANY",
            "targetEntity": "product",
            "fetchType": "LAZY",
            "cascadeType": "ALL"
          }
        }
      ]
    }
  ]
}